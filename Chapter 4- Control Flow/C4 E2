#if
alien_color = 'green'

if alien_color == 'green':
    print('You just earned 5 points for shooting the alien.')
else:
    print ('\nYou  just earned 10 points.')

#else
alien_color = 'red'

if alien_color == 'green':
    print('You just earned 5 points for shooting the alien.')
else:
    print ('\nYou  just earned 10 points.')