places = ["AbuDhabi", "Dubai", "Peshawer", "Paris", "LA"]
print ("\nRaw form:\n", places)

print("\nAlphabetical order:\n",sorted(places))

print("\nOriginal order:\n", places)

print("\nReverse alphabetical order:\n", sorted(places, reverse=True))

print("\nOriginal order:\n", places)

places.reverse()
print("\nReverse:\n", places)

places.reverse()
print("\nOriginal:\n", places)

places.sort()
print("\nAlphabetical:\n", places)

places.sort(reverse=True)
print("\nReverse alphabetical order:\n", places)